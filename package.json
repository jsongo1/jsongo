{
  "name": "jsongo",
  "version": "0.6.0",
  "description": "Like MongoDB, except stores data in git-friendly flat json files",
  "license": "MIT",
  "bin": "./cli/jsongo-cli.js",
  "main": "./lib/jsongo.js",
  "scripts": {
    "build": "tsc",
    "test": "yarn build && ava",
    "watch": "(tsc --watch &); ava --watch"
  },
  "dependencies": {
    "bson-objectid": "^1.3.1",
    "json-validator-duplicated-keys": "^1.0.1",
    "mingo": "^3.0.6",
    "sort-keys": "^4.0.0",
    "value-or-json": "^0.1.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/yargs": "^15.0.5",
    "ava": "^3.12.1",
    "memfs": "^3.2.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "cli/*.ts",
    "lib/*.ts"
  ],
  "keywords": [
    "json",
    "mongo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rentzsch/jsongo.git"
  },
  "bugs": {
    "url": "https://github.com/rentzsch/jsongo/issues"
  }
}
